<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<parameter name="username" value="admin"></parameter>
<parameter name="password" value="123"></parameter>
<test name="ChromeTest">
	<parameter name="browsers" value="chrome"></parameter>
	<classes>
		<class name="testscript.LoginPageTest">
			<methods>
				<include name="verifyUsercanloginWithINValidUsernames"></include>
			</methods>
		</class>

	</classes>
</test>
<test name="EdgeTest">
	<parameter name="browsers" value="edge"></parameter>
	<classes>
		<class name="testscript.LoginPageTest">
			<methods>
				<include name="verifyUsercanloginWithINValidUsernames"></include>
			</methods>
		</class>

	</classes>
</test>
</suite>
